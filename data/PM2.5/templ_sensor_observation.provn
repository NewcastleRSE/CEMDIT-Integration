document
  default <http://www.ncl.ac.uk/>
  prefix vargen <http://openprovenance.org/vargen#>
  prefix var <http://openprovenance.org/var#>
  prefix sp <http://www.w3.org/ns/sosa/prov/>
  prefix sosa <http://www.w3.org/ns/sosa/>

  bundle vargen:templ_observation
    default <http://www.ncl.ac.uk/>
    prefix vargen <http://openprovenance.org/vargen#>
    prefix var <http://openprovenance.org/var#>

    entity(sosa:Result, [value='var:value'])
    entity(sosa:Procedure, [value='var:template'])
    entity(sosa:FeatureOfInterest, [value='var:theme', value='var:units'])
    activity(sosa:Observation, [value='var:timestamp'])
    used(sosa:Observation, sosa:FeatureOfInterest, -)
    used(sosa:Observation, sosa:Procedure, -)
    wasAssociatedWith(sosa:Observation, sosa:Sensor, -)
    wasGeneratedBy(sosa:Result, sosa:Observation, -)
    agent(sosa:Sensor, [value='var:sensorname'])
  endBundle
endDocument
